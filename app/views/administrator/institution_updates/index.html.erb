<h1>Listing institution_updates</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Institution</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @institution_updates.each do |institution_update| %>
  <tr>
    <td><%= institution_update.title %></td>
    <td><%= institution_update.content %></td>
    <td><%= institution_update.institution.name %></td>
    <td><%= link_to 'Show', [:administrator, institution_update] %></td>
    <td><%= link_to 'Edit', edit_administrator_institution_update_path(institution_update) %></td>
    <td><%= link_to 'Destroy', [:administrator, institution_update], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

