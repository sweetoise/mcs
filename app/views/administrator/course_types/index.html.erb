<h1>Listing course_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @course_types.each do |course_type| %>
  <tr>
    <td><%= course_type.name %></td>
    <td><%= link_to 'Show', [:administrator, course_type] %></td>
    <td><%= link_to 'Edit', edit_administrator_course_type_path(course_type) %></td>
    <td><%= link_to 'Destroy', [:administrator, course_type], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

