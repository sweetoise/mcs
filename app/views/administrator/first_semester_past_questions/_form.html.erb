<%= simple_form_for([:administrator, @first_semester_past_question]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :year %>
    <%= f.input :solution_url %>
  </div>
    <div class="field">
	  <%= f.label :first_semester_course_id %><br />
	  <%= f.collection_select :first_semester_course_id, FirstSemesterCourse.order(:code), :id, :code, include_blank: true, :prompt => "- Select First Semester Course -" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
