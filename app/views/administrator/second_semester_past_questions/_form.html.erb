<%= simple_form_for([:administrator, @second_semester_past_question]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :year %>
    <%= f.input :solution_url %>
  </div>

    <div class="field">
	  <%= f.label :second_semester_course_id %><br />
	  <%= f.collection_select :second_semester_course_id, SecondSemesterCourse.order(:code), :id, :code, include_blank: true, :prompt => "- Select Second Semester Course -" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
