<%= simple_form_for([:administrator, @second_semester_library_book_outline]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :topic %>
    <%= f.input :content %>
    <%= f.input :book_url %>
  </div>
  <div class="field">
	  <%= f.label :second_semester_library_book_id %><br />
	  <%= f.collection_select :second_semester_library_book_id, SecondSemesterLibraryBook.order(:name), :id, :name, include_blank: true, :prompt => "- Select Second Semester Library Book -" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
