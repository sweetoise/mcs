<h1>Listing faculty_updates</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Date</th>
    <th>Signed by</th>
    <th>Faculty</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @faculty_updates.each do |faculty_update| %>
  <tr>
    <td><%= faculty_update.title %></td>
    <td><%= faculty_update.content %></td>
    <td><%= faculty_update.date %></td>
    <td><%= faculty_update.signed_by %></td>
    <td><%= faculty_update.faculty_id %></td>
    <td><%= link_to 'Show', [:administrator, faculty_update] %></td>
    <td><%= link_to 'Edit', edit_administrator_faculty_update_path(faculty_update) %></td>
    <td><%= link_to 'Destroy', [:administrator, faculty_update], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

