<h1>Listing department_notices</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Date</th>
    <th>Department</th>
    <th>Signed by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @department_notices.each do |department_notice| %>
  <tr>
    <td><%= department_notice.title %></td>
    <td><%= department_notice.content %></td>
    <td><%= department_notice.date %></td>
    <td><%= department_notice.department.name %></td>
    <td><%= department_notice.signed_by %></td>
    <td><%= link_to 'Show', [:administrator, department_notice] %></td>
    <td><%= link_to 'Edit', edit_administrator_department_notice_path(department_notice) %></td>
    <td><%= link_to 'Destroy', [:administrator, department_notice] , method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

