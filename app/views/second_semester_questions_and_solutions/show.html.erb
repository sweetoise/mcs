<div class="subscription-nav-links">
	<table border="0">
		<tr>
			<td>
				<div class="solution_bank_back-to-course">
					<%= link_to @second_semester_past_question.second_semester_course.code, :controller => 'user_second_semester_course', :action => 'show', :id => @second_semester_past_question.second_semester_course_id %>
			    </div> 
			</td>
			<td>
				<div class="solution_bank_back-to-library">Back to: &nbsp;&nbsp;
					<%= link_to 'Solution bank', :controller => 'questions_and_solutions', :action => 'index' %>
				</div>
			</td>
		</tr>
	</table>
</div>
<div class="show-solution-year-heading">
     <%= @second_semester_past_question.second_semester_course.code %> - Examination Solutions (<%= @second_semester_past_question.year %>)
</div>

    <div class="show_first_semester_course_outline_details_solution_year">
    	<table>
    		<tr>
    			<td><b style="color: #3e3e3e; margin-left: 20px;">Solutions Years: </b></td>
    		    <% current_user.second_semester_course.second_semester_past_questions.order('year DESC').each do |list_past_question_years| %>
