    				<div class="show-solution-years-list">
    			        <%= link_to list_past_question_years.year, :controller => 'past_questions_update_second_semester_course', :action => 'show', :id => list_past_question_years %>
    	            </div>
    			</td>
    			<% end %>
    		</tr>
    	</table>
    </div>
       <div style="width: 650px; padding-left: 70px; margin-top: 40px; text-align: center; font-weight: bold; font-size: 14px; color: #4e4e4e; margin-bottom: 10px;">
      	   <%= @second_semester_past_question.second_semester_course.title %>.
       </div>
       <div class="solution_bank_course_outline_url_view">
      	 <iframe src="<%= @second_semester_past_question.solution_url  %>" scrollbars="auto" style="min-width:890px; height:1105px;" scrollbars="no"; frameborder="0"></iframe>
       </div>






