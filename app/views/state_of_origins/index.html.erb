<h1>Listing state_of_origins</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @state_of_origins.each do |state_of_origin| %>
  <tr>
    <td><%= state_of_origin.name %></td>
    <td><%= link_to 'Show', state_of_origin %></td>
    <td><%= link_to 'Edit', edit_state_of_origin_path(state_of_origin) %></td>
    <td><%= link_to 'Destroy', state_of_origin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

